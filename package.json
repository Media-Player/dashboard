{
  "name": "dashboard",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "repository": "git@github.com:Media-Player/dashboard.git",
  "author": "Ivan Guimar√£es Monte <ivg.monte@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "build:prod": "npm run type:check && npm run build",
    "lint": "./node_modules/.bin/eslint src/**/*.ts --fix",
    "prettier": "./node_modules/.bin/prettier --config ./.prettierrc.js",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "start": "npm run build && electron ./dist/src/index.js",
    "start:debug": "npm run build && NODE_ENV=development node -r dotenv/config -r ./tsconfig-paths-bootstrap.js --nolazy --inspect-brk=9229 dist/src/index.js",
    "start:dev": "NODE_ENV=development node -r dotenv/config -r ./tsconfig-paths-bootstrap.js dist/src/index.js",
    "start:watch": "NODE_ENV=dev nodemon --exec \"electron dist/src/index.js\" && tsc ./src/index.ts -w",
    "type:check": "./node_modules/.bin/tsc -p . --noEmit"
  },
  "devDependencies": {
    "@types/electron": "1.6.10",
    "@types/node": "13.13.5",
    "@typescript-eslint/eslint-plugin": "2.32.0",
    "@typescript-eslint/parser": "2.32.0",
    "dotenv": "8.2.0",
    "dotenv-cli": "3.1.0",
    "electron": "9.4.0",
    "electron-is-dev": "1.2.0",
    "electron-reload": "1.5.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "nodemon": "2.0.3",
    "prettier": "2.0.5",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  }
}